{
  "source": {
    "include": [
      "src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
    "babelrc": false,
    "extensions": ["ts"],
    "presets": ["@babel/preset-typescript"]
  },
  "templates": {
    "referenceTitle": "Geoblink Lodash Mixins",
    "disableSort": true
  },
  "opts": {
    "template": "./node_modules/jsdoc-template",
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true
  }
}