{
  "source": {
    "include": [
      "src",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "referenceTitle": "Geoblink Lodash Mixins",
    "disableSort": true
  },
  "opts": {
    "template": "./node_modules/jsdoc-template",
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true
  }
}